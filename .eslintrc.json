{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "Dialog": "readonly",
    "Helper": "readonly",
    "eLabSDK": "readonly",
    "Class": "readonly",
    "Options": "readonly",
    "Events": "readonly"
  },
  "rules": {
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "import/extensions": "off",
    "func-names": "off",
    "no-extra-boolean-cast": "off",
    "no-restricted-syntax": "off",
    "arrow-body-style": "off",
    "operator-assignment": "off",
    "prefer-destructuring": "off",
    "no-plusplus": "off"
  },
  "overrides": [
    {
      "files": [
        "./src/date.js"
      ],
      "rules": {
        "import/prefer-default-export": "off"
      }
    }
  ]
}